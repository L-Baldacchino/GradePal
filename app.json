{
  "expo": {
    "name": "Grade Pal",
    "slug": "ExamCalculator",
    "version": "1.0.4",
    "orientation": "portrait",
    "scheme": "examcalculator",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1A2F"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "4"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#141A26"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.l.baldacchino.GradePal",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE"
      ],
      "blockedPermissions": ["android.permission.RECORD_AUDIO"]
    },
    
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#141A26",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/splash.png",
            "resizeMode": "contain"
          }
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5536d06d-77ae-40f0-9918-f42b89b027cb"
      }
    },
    "owner": "l.baldacchino"
  }
}
